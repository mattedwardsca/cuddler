@using global::Cuddler.Core.Utils
@using Kendo.Mvc.UI
@model object
@{
    var webId = WebIdUtil.GetWebId();
    var employees = new[] { "WIP", "Client hold", "Factory hold", "Sales", "Accepted", "DONE" };
}
<script>
    function @(webId)(e) { e.sender.open(); }
</script>
@(Html.Kendo()
      .DropDownListFor(m => m)
      .BindTo(employees)
      .Events(e => e.DataBound(webId)))
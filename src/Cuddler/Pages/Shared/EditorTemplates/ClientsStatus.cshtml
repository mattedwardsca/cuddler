@using global::Cuddler.Utils
@using Kendo.Mvc.UI
@model object
@{
    var webId = WebIdUtil.GetWebId();
    var employees = new[] { "New Project", "Bidding", "Accepted" };
}
<script>
    function @(webId)(e) { e.sender.open(); }
</script>
@(Html.Kendo()
      .DropDownListFor(m => m)
      .BindTo(employees)
      .Events(e => e.DataBound(webId)))
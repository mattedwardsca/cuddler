@using global::Cuddler.Web.ViewModels
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model PageViewTagHelper

@{
    switch (Model.ViewModel.Template)
    {
        case ECuddlerTemplate.Master:

            <partial model="Model"
                     name="~/Pages/Shared/Cuddler/PageView/Master.cshtml" />

            break;
        default:
            throw new InvalidOperationException($"The ViewModel template '{nameof(Model.ViewModel.Template)}' has not been implemented. Error: daa37a74-605c-4147-9a04-718e3f53819f");
    }
}